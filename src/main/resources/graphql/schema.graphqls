type Query {
        todos: [Todo]
}

type Mutation {
        addTodo(id: ID!,title: String!,desc: String!,status: String!): Todo
}

input TodoInput {
        id: ID!
        title: String!
        desc: String!
        status: String!
}

type Todo {
        id: ID!
        title: String!
        desc: String!
        status: String!
}
